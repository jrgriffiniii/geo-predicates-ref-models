myGazetteer:exampleBoundingBox1 a schema:GeoShape ;
  schema:box "45.15, -93.85" .

myGazetteer:examplePoint1 a schema:GeoCoordinates ;
  schema:latitude 40.75 ;
  schema:longitude 73.98 .

# See http://www.opengeospatial.org/standards/sfa to reference the Well-known text format
myGazetteer:examplePolygon1 a schema:GeoShape ;
  schema:polygon "POLYGON((30 10, 40 40, 20 40, 10 20, 30 10))" .

myGazetteer:examplePath1 a schema:GeoShape ;
  schema:line "LINESTRING(30 10, 10 30, 40 40)" .

# Define the toponyms as a gazetteer

myGazetteer:examplePlace1 a schema:Place ;
  schema:name "Example Place 1"@en ;
  schema:geo  myGazetteer:exampleBoundingBox1 .

myGazetteer:examplePlace2 a schema:Place ;
  schema:name "Example Place 2"@en ;
  schema:geo  myGazetteer:examplePoint1 .

myGazetteer:examplePlace3 a schema:Place ;
  schema:name "Example Place 3"@en ;
  schema:geo  myGazetteer:examplePolygon1 .

myGazetteer:examplePlace4 a schema:Place ;
  schema:name "Example Place 4"@en ;
  schema:geo  myGazetteer:examplePath1 .

testRepository:exampleScannedMap a PCDM:Object, schema:Map ;
  dcterms:spatial    testRepository:examplePlace1 ;
  dcterms:description    "An Example Scanned Map"@en ;
  schema:spatialCoverage    testRepository:examplePlace1 ;
  ebucore:hasMimeType    "image/tiff" .
